  "scripts": {
    "start": "cross-env NODE_ENV=production node ./server/dist/index.js",
    "build": "cross-env NODE_ENV=development babel ./server/ -d ./server/dist",
    "heroku-postbuild": "cross-env NODE_ENV=production babel ./server/ -d ./server/dist",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint \"server/**/*.js\" index.js --fix",
    "test": "cross-env NODE_ENV=test nyc mocha ./server/tests/**/'*.test.js' --recursive --timeout 50000 --exit",
    "test:watch": "npm test --watch"
  },
  "dependencies": {
    "express": "^4.16.3"

  },
